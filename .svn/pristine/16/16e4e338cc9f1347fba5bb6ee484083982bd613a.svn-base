package com.flf.entity;

import java.util.List;

import javax.xml.bind.annotation.XmlRootElement;

/**
 * 评分项表
 * @author shaozheng
 *	2015-7-8
 */
@XmlRootElement(name = "ScoreCriteriaItems")
public class ScoreCriteriaItems {
	
	
	private String criteriaItemId;//评分项id
	private String criteriaItemName;//评分项名称
	private String criteriaId;//评分标准id
	private String number;//评分项序号
	private String weight;//权重
	
	private List<ScoreSubitemItems> scoreSubitemItemsList;
	
	public String getCriteriaItemId() {
		return criteriaItemId;
	}
	public void setCriteriaItemId(String criteriaItemId) {
		this.criteriaItemId = criteriaItemId;
	}
	public String getCriteriaItemName() {
		return criteriaItemName;
	}
	public void setCriteriaItemName(String criteriaItemName) {
		this.criteriaItemName = criteriaItemName;
	}
	public String getCriteriaId() {
		return criteriaId;
	}
	public void setCriteriaId(String criteriaId) {
		this.criteriaId = criteriaId;
	}
	public String getNumber() {
		return number;
	}
	public void setNumber(String number) {
		this.number = number;
	}
	public String getWeight() {
		return weight;
	}
	public void setWeight(String weight) {
		this.weight = weight;
	}
	public List<ScoreSubitemItems> getScoreSubitemItemsList() {
		return scoreSubitemItemsList;
	}
	public void setScoreSubitemItemsList(
			List<ScoreSubitemItems> scoreSubitemItemsList) {
		this.scoreSubitemItemsList = scoreSubitemItemsList;
	}
}
