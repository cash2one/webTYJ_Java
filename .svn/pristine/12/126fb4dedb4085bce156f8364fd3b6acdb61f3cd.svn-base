package com.flf.entity;



import javax.xml.bind.annotation.XmlRootElement;

/**
 * 装修单位关联记录
 * @author taoyongchao
 *2015-8-17
 */
@XmlRootElement(name = "DecorateAssociatedRecords")
public class DecorateAssociatedRecords {
	private String recordId;// 装修记录id
	private String applyId; //关联装修申请ID
	private String decorateId;  //装修公司id
	private String headId;    //现场负责人id
//	private List<OwnerRenovationApply> OwnerRenovationApply; 
	
	public String getRecordId() {
		return recordId;
	}
	public void setRecordId(String recordId) {
		this.recordId = recordId;
	}
	public String getApplyId() {
		return applyId;
	}
	public void setApplyId(String applyId) {
		this.applyId = applyId;
	}
	public String getDecorateId() {
		return decorateId;
	}
	public void setDecorateId(String decorateId) {
		this.decorateId = decorateId;
	}
	public String getHeadId() {
		return headId;
	}
	public void setHeadId(String headId) {
		this.headId = headId;
	}
	
	

}
