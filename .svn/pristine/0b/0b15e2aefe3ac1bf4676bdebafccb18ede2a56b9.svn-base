<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>新增专业(专业资产初始化)</title>
    <link href="css/internalProfession/main.css" rel="stylesheet">
</head>
<body ng-controller="newProfessionalCtrl">

<div class="col-lg-12 col-md-12 col-sm-6">
    <div class="row">
        <div class="contract_panner">
            <!--合同内容模板列表-->
            <!--创建合同编辑框开始-->
            <div class="row">
                <div class="row table-padding ">
                    <div class="contract_newinfo">
                        <table width="100%">
                            <tr>
                                <td width="80"> 专业名称： </td>
                                <td><form><input name="input" type="text" class="professional_input" ng-model="PropertyType.propertyTypeName" /></form></td>
                                <td width="80"> 专业编号： </td>
                                <td><form><input name="input" type="text" class="professional_input" ng-model="PropertyType.propertyTypeNumber" /></form></td>
                                <td width="80"> 父级编号： </td>
                                <td><form><input name="input" type="text" class="professional_input" /></form></td>
                                <td width="160">
                                    <input type="button"  class="professional_select" value="选择父级">
                                    <input type="button"  class="professional_select" value="追加专业">
                                </td>
                            </tr>
                        </table>
                    </div>
                </div>
            </div>
            <!--创建合同编辑框结束-->
            <div class="row inside_padding">
                <div class="contract_table">
                    <div class="professional_table_height">
                        <table width="100%">
                            <tr>
                                <td align="center" width="16%" height="42px">
                                    专业编号
                                </td>
                                <td align="center" width="16%" height="42px" >
                                    <span >专业名称</span>
                                </td>
                                <td align="center" width="16%" height="42px">
                                    <span >创建人</span>
                                </td>
                                <td align="center" width="16%" height="42px">
                                    <span >创建时间</span>
                                </td>
                                <td align="center" width="16%" height="42px">
                                    <span >修改人</span>
                                </td>
                                <td align="center" width="20%" height="42px">
                                    <span >修改时间</span>
                                </td>
                            </tr>
                        </table>
                        <table width="100%" >
                            <tr ng-repeat="items in propertyTypes">
                                <td  width="16%" height="42px" align="center">
                                    {{items.propertyTypeNumber}}
                                </td>
                                <td  width="16%" height="42px" align="center">
                                    <span >{{items.propertyTypeName}}</span>
                                </td>
                                <td  width="16%" height="42px" align="center">
                                    <span >{{items.founder}}</span>
                                </td>
                                <td  width="16%" height="42px" align="center">
                                    <span >{{items.founderTime | date:"yyyy-MM-dd" }}</span>
                                </td>
                                <td  width="16%" height="42px" align="center">
                                    <span ></span>
                                </td>
                                <td  width="20%" height="42px" align="center">
                                    <span ></span>
                                </td>
                            </tr>

                        </table>
                    </div>
                    <input type="button"  class="authorizationb" ng-click="addPropertyType()" value="保存">
                </div>
            </div>
        </div>
        <!--合同内容模表结束-->
    </div>
</div>

