package com.flf.entity;

import javax.xml.bind.annotation.XmlRootElement;
//收费项
@XmlRootElement(name = "ChargeItem") 
public class ChargeItem {
    private String chargeItemId;//收费项主键

    private String chargeItemType;//收费类型

    private Double chargeItemArea;//收费面积

    private Double unitPrice;//单价

    private Double money;//金额

    private String chargeUnit;//收费单位（年/月）
    
    private String pactId;//物业合同id

    public String getPactId() {
		return pactId;
	}

	public void setPactId(String pactId) {
		this.pactId = pactId;
	}

	public String getChargeItemId() {
        return chargeItemId;
    }

    public void setChargeItemId(String chargeItemId) {
        this.chargeItemId = chargeItemId;
    }

    public String getChargeItemType() {
        return chargeItemType;
    }

    public void setChargeItemType(String chargeItemType) {
        this.chargeItemType = chargeItemType;
    }

    public Double getChargeItemArea() {
        return chargeItemArea;
    }

    public void setChargeItemArea(Double chargeItemArea) {
        this.chargeItemArea = chargeItemArea;
    }

    public Double getUnitPrice() {
        return unitPrice;
    }

    public void setUnitPrice(Double unitPrice) {
        this.unitPrice = unitPrice;
    }

    public Double getMoney() {
        return money;
    }

    public void setMoney(Double money) {
        this.money = money;
    }

    public String getChargeUnit() {
        return chargeUnit;
    }

    public void setChargeUnit(String chargeUnit) {
        this.chargeUnit = chargeUnit;
    }
}