html, body{height:100%;}
body{
    background:#ccc; margin:0;
    font-family: Microsoft YaHei, Arial, sans-serif;
    line-height:24px;
    color:#313131;
    font-size:12px;
}
.font14{ font-size:14px;}
.font16{ font-size:16px;}
.fontBold{ font-weight:bold;}
a{ text-decoration:none;}
a:hover{ text-decoration:none;}
ul, li{
    list-style-type: none;
}
.clear{ clear:both;}

/*头部样式文件*/
.header{ height:50px; border-bottom:2px solid #f89c35; background:#313131; margin-top:0;}
.logo{ width:126px; height:50px; float:left; background:url(../images/LOGO1.jpg) left top no-repeat; margin-left:10px; margin-top:3px;}
.set{ height:40px; width:120px; float:right; margin-right:10px; margin-top:5px;}
.set_button{ display:block; width:20px; height:20px; background:url(../images/seta.png) center top no-repeat; margin-top:14px; float:left;}
.set_button:hover{ display:block; width:20px; height:20px; background:url(../images/set.png) center top no-repeat; margin-top:14px; float:left;}
.set_start{display:block; float:left; margin-top:10px;margin-left: 10px;}
.set_start a{ color:#FFF; text-decoration:none; }
.set_start a:hover{ color:#ddd; text-decoration:none;}
.set_close { display:block; margin-left:10px; float:left; margin-top:10px;}
.set_close a{ color:#FFF; text-decoration:none; }
.set_close a:hover{ color:#ddd; text-decoration:none;}



/*左侧样式*/
.left{
