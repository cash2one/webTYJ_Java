<body ng-controller="chooseHouseCtrl">
<div class="container" style="padding: 10px 10px 0;width: 97%;margin-top: 50px" ng-repeat="items in House" >
    <div class="media-right">
        <div class="row">  
            <div class="media">
                <div class="media-left">
                    <a href="#">
                        <img src="images/propertyService/choiceHousing/u81.jpg" width="50px;" height="50px" style="margin-top: 5px;">
                    </a>
                </div>
                <div class="media-body" >
                    <div class="row">
                        <div class="col-md-4">房屋地址：{{items.houseAddress}}</div>
                        <div class="col-md-4">业主姓名：{{}}</div>
                        <div class="col-md-offset-9">
                            <a href="#" data-toggle="modal"  ng-click="getHouseId(items.houseId)"  data-target="#showModal">
                                <span class="glyphicon glyphicon-search btn" aria-hidden="true"></span>
                            </a>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2">房屋编码：{{items.houseId}}</div>
                        <div class="col-md-2"  ng-switch on="items.joinState">入伙状态：
                        <span ng-switch-when="1">已入伙</span>
                         <span ng-switch-when="2">没入伙</span>
                        
                        </div>
                        <div class="col-md-2">竣工面积:{{items.completeArea}}</div>
                        <div class="col-md-4">房屋户型:{{items.houseTypeName}}</div>
                    </div>
                </div>
            </div>
        </div>
       
