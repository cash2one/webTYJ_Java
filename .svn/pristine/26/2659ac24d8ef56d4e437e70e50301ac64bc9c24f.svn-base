    -moz-border-radius:5px;
    -webkit-border-radius:5px;
    border-radius:5px;
    height:585px;
    margin:10px auto;
    position:relative;
}


/*左侧用户头像和名称样式*/
.user{ padding:10px 10px 10px 20px; margin-bottom:10px; min-height:80px;}
.user_img{ width:55px; height:58px; float:left;}
.user_info{ height:58px; float:left; margin-left:5px; margin-top:5px;}
.user_number{ display:block; width:15px; height:15px; background:#F00; text-align:center; line-height:15px; color:#fff;-moz-border-radius:10px; -webkit-border-radius:10px; border-radius:10px; float:right;}
.user_info a{ color:#03a0f1;}
.user_info a:hover{ color:#313131;}

/*左侧搜索框样式*/
.search{}
.search_input{ width:80%; float:left; height:40px; background:#ddd; border:none; padding-left:10px;border-bottom:1px solid #a4a4a4; border-top:1px solid #a4a4a4; }
.search_button{ width:20%; float:left; height:40px; background:#ddd url(../images/search.png) center 5px no-repeat; border:none;border-bottom:1px solid #a4a4a4; border-top:1px solid #a4a4a4; }

