                <li><button type="button" class="btn btn-default btn-xs" id="xz">全部</button></li>
                <li ng-repeat="button in buttons">
                    <button type="button" class="btn btn-default btn-xs" data-toggle="button">
                        {{button.content}}
                    </button>
                </li>
                <li >
                    <div class="checkbox" >
                        <label><input type="checkbox">多选</label>
                    </div>
                </li>
            </ul>
        </div>
