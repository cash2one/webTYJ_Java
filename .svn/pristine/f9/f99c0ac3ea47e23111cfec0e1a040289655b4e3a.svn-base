<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>模板详情</title>
</head>
<body>

<div class="complaint_user_list" style="margin: 10px 10px;border: 1px solid #000000;width: 97%">
    <div  style="margin-top: 10px">
        <div style="border: 1px solid ;width: 80px;height: 80px;margin: 15px 20px" >
            <div style="margin-top: 25px;margin-left: 10px">
                <span style="font-size: 30px;">模板</span>
            </div>
        </div>
        <div class="col-md-12">
            <div class="row" style="margin-left: 100px;margin-top: -90px">
                <div class="col-md-6">表名称：{{template.name}}</div>
                <div class="col-md-6">描述：{{template.describe}}</div>
            </div>
            <div class="row" style="margin-left: 100px;margin-top: -80px">
                <div class="col-md-3">表分类：{{template.classify}}</div>
                <div class="col-md-3">周期：{{template.weeks}} </div>
                <div class="col-md-4">岗位：{{template.post}}</div>
            </div>
        </div>
    </div>
</div>
<div style="border: 1px solid #000000;width: 97%;height:400px;margin: 10px">
    <table border="1" style="width: 90% ;margin:20px auto;text-align: center">
        <tr>
            <td>{{thead.title1}}</td>
            <td>{{thead.title2}}</td>
            <td>{{thead.title3}}</td>
            <td>{{thead.title4}}</td>
            <td>{{thead.title5}}</td>
            <td>{{thead.title6}}</td>
            <td>{{thead.title7}}</td>
            <td>{{thead.title8}}</td>
        </tr>

        <tr>
            <th rowspan="10" style="text-align: center">{{item.td1}}</th>
            <th rowspan="10" style="text-align: center">{{item.td2}}</th>
            <td rowspan="5" style="text-align: center">{{item.td3}}</td>
            <td rowspan="5" style="text-align: center">{{item.td4}}</td>
            <td >{{item.td5}}</td>
            <td>{{item.td6}}</td>
            <td>{{item.td7}}</td>
            <th rowspan="10" style="text-align: center">{{item.td8}}</th>
        </tr>
        <tr>
            <td >{{item1.td1}}</td>
            <td>{{item1.td2}}</td>
            <td>{{item1.td3}}</td>
        </tr>
        <tr>
            <td >{{item2.td1}}</td>
            <td>{{item2.td2}}</td>
            <td>{{item2.td3}}</td>
        </tr>
        <tr>
            <td >{{item3.td1}}</td>
            <td>{{item3.td2}}</td>
            <td>{{item3.td3}}</td>
        </tr>
        <tr >
            <td >{{item4.td1}}</td>
            <td>{{item4.td2}}</td>
            <td>{{item4.td3}}</td>
        </tr>
        <tr >
            <td rowspan="5" style="text-align: center">{{item5.td4}}</td>
            <td rowspan="5" style="text-align: center">{{item5.td5}}</td>
            <td >{{item5.td1}}</td>
            <td>{{item5.td2}}</td>
            <td>{{item5.td3}}</td>
        </tr>
        <tr>
            <td >{{item6.td1}}</td>
            <td>{{item6.td2}}</td>
            <td>{{item6.td3}}</td>
        </tr>
        <tr>
            <td >{{item7.td1}}</td>
            <td>{{item7.td2}}</td>
            <td>{{item7.td3}}</td>
        </tr>
        <tr>
            <td >{{item8.td1}}</td>
            <td>{{item8.td2}}</td>
            <td>{{item8.td3}}</td>
        </tr>
        <tr>
            <td >{{item9.td1}}</td>
            <td>{{item9.td2}}</td>
            <td>{{item9.td3}}</td>
        </tr>
    </table>
    <div class="row">
        <div ui-view></div>
    </div>
</div>
</body>
</html>



