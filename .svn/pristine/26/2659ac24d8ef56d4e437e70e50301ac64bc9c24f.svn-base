}

.nav-btn{
    width:43px;
    height:45px;
    float:left;
    margin-right:6.5px;
}


.navigation{
    width:100%;
    margin:10px auto 0 auto;
    height:32px;
}
.navigation a{
    float:left;
    display:block;
    color:#fff;
    font-size:13px;
    margin-right:2px;
    text-decoration:none;
    font-weight:bold;
    height:30px;
    padding:0 25px;
    background:#939393;
    border-top:5px solid #939393;
    border-left:1px solid #939393;
    border-right:1px solid #939393;
    border-bottom:1px solid #939393;
    -moz-border-radius:4px 4px 0 0;
    -webkit-border-radius:4px 4px 0 0;
    border-radius:4px 4px 0 0;
}
.navigation .navigation_this{
    color:#5d5d5d;
    background:#dddddd;
    border-left:1px solid #bbbbbb;
    border-right:1px solid #bbbbbb;
    border-top:5px solid #f49c34;
    border-bottom:1px solid #dddddd;
}
.navigation a:hover{
    color:#5d5d5d;
    background:#dddddd;
    border-left:1px solid #bbbbbb;
    border-right:1px solid #bbbbbb;
    border-top:5px solid #f49c34;
    border-bottom:1px solid #dddddd;
}


/*图框属性*/
.panel{
    margin:0;
    padding:0;
    margin-left:-15px;
    margin-bottom:15px;
    background:#f0f0f0;
    border:1px solid #bbbbbb;
    height:235px;
    overflow:hidden;
    font-size:14px;
