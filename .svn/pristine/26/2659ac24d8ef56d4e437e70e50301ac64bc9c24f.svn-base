    width:16px; height:16px;
    float:left;
    margin-top:6px;
}
.close_button {
    background:url(../images/close.jpg) center top no-repeat;
    display:block;
    width:16px; height:16px;
    float:left;
    margin-top:6px;
    margin-left:10px;
}




.dot{ height:20px; text-align:center; width:80px; margin:0 auto; }

