            <div class="pull-left col-md-4 form-group">
                <h5 >搜&nbsp;&nbsp;索</h5>
            </div>
            <div class="form-group col-md-6">
                <input type="text" class="form-control" style="width: 350px" placeholder="search">
                <div type="button" class=" btn btn-default form-group">
                    <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
                </div>
            </div>

            <div class="col-md-2 form-group" style="margin-top: 10px">
                展开
                    <span class="glyphicon" ng-class="vm.collapsed?'glyphicon-chevron-up':'glyphicon-chevron-down'"
