package com.flf.entity;

import javax.xml.bind.annotation.XmlRootElement;

/**
 * 方案收费项目关系表
 * @author wangzhou
 *
 */
@XmlRootElement(name = "SchemeRelation")
public class SchemeRelation {
	
	//主键id
	private String schemeRelationId;
	
	//方案id
	private String schemeId;
	
	//收费项目id
	private String chargeItemId;
	
	//关联收费项目实体类
	private ChargeItemNew chargeItemNew;

	public String getSchemeRelationId() {
		return schemeRelationId;
	}

	public void setSchemeRelationId(String schemeRelationId) {
		this.schemeRelationId = schemeRelationId;
	}

	public String getSchemeId() {
		return schemeId;
	}

	public void setSchemeId(String schemeId) {
		this.schemeId = schemeId;
	}

	public String getChargeItemId() {
		return chargeItemId;
	}

	public void setChargeItemId(String chargeItemId) {
		this.chargeItemId = chargeItemId;
	}

	public ChargeItemNew getChargeItemNew() {
		return chargeItemNew;
	}

	public void setChargeItemNew(ChargeItemNew chargeItemNew) {
		this.chargeItemNew = chargeItemNew;
	}
	
}