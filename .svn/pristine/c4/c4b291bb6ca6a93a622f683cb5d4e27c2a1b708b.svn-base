                                        <td><div class="input-group"><label class="col-md-6" for="date5">免租开始时间：</label><div class="col-md-6"><input type="text" class="form-control" id="date5"></div></div></td>
                                        <td> <div class="input-group"><label class="col-md-6" for="date6">免租结束时间：</label><div class="col-md-6"><input type="text" class="form-control" id="date6"></div></div></td>
                                    </tr>
                                </table>
                            </div>
                        </div>



                        <div style="padding:20px;" align="right">
                            <button  type="button" class="btn btn-primary" data-dismiss="modal">提交</button>
                            <button type="button" class="btn btn-primary">取消</button>
                            <button type="button" class="btn btn-primary">打印</button>
                        </div>
                    </div><!-- /.modal-content -->
                </div><!-- /.modal -->
            </div>
    <div class="clear"></div>
</div>

</div>

