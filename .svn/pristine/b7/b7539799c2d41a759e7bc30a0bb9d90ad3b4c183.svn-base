    <!--模态框-->
    <div class="modal fade" id="modifyPete" role="dialog" aria-label="myModalLabel" aria-hidden="true" >
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header" style="background-color: white;height: 50px">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <a ui-sref="index.propertyService.staffHome.entranceGuard.addEntranceGuard"><span style="color: #000000">新增门禁卡</span></a>
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="media-left" style="height:auto;width: 600px;">
                        <div class="col-md-10">
                            <div class="row" style="padding-top:15px">
                                <p class="col-md-4">ID卡号：{{icc}}</p>
                                <p class="col-md-4">启用日期：{{commissioning}}</p>
                                <p class="col-md-4">操作人：{{personal}}</p>
                            </div>
                            <div class="row">
                                <p class="col-md-4">卡状态：{{condition}}</p>
                                <p class="col-md-4">截止日期：{{deadline}}</p>
                                <p class="col-md-4">办卡日期：{{born}}</p>
                            </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn bg-primary" data-dismiss="modal" aria-label="Close">关闭</button>
                </div>

            </div>
        </div>
    </div>
</div>
</div>



