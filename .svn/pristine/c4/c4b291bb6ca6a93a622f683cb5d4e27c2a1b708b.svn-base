                                <table  class="table5">
                                    <tr >
                                        <td><div class="input-group "><label class="col-md-6 " for="no">房屋编号：</label><div class="col-md-6"><input type="text" class="form-control" id="no" ></div></div></td>
                                        <td ><div class="input-group"><label class="col-md-6" for="time1">出租开始日期：</label><div class="col-md-6"><input type="datetime" class="form-control" id="time1"></div></div></td>
