package com.flf.entity;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement(name="PlanLinkBuildingStructure")
public class PlanLinkBuildingStructure {

	private String planLinkBuildingStructureId;
	
	private String buildingStructureId;
	
	private String meterReadingProgramId;

	public String getPlanLinkBuildingStructureId() {
		return planLinkBuildingStructureId;
	}

	public void setPlanLinkBuildingStructureId(String planLinkBuildingStructureId) {
		this.planLinkBuildingStructureId = planLinkBuildingStructureId;
	}

	public String getBuildingStructureId() {
		return buildingStructureId;
	}

	public void setBuildingStructureId(String buildingStructureId) {
		this.buildingStructureId = buildingStructureId;
	}

	public String getMeterReadingProgramId() {
		return meterReadingProgramId;
	}

	public void setMeterReadingProgramId(String meterReadingProgramId) {
		this.meterReadingProgramId = meterReadingProgramId;
	}
	
	
}
