package com.flf.util;

import java.util.ArrayList;
import java.util.List;

public class Tree {
	
	public String id;
	public String pid;
	public String name;
	public List<Tree> tList = new ArrayList<Tree>();
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getPid() {
		return pid;
	}
	public void setPid(String pid) {
		this.pid = pid;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public List<Tree> gettList() {
		return tList;
	}
	public void settList(List<Tree> tList) {
		this.tList = tList;
	}
	
	

}
