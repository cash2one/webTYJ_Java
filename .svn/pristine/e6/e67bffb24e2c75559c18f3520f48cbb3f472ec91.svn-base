package com.flf.entity;

import javax.xml.bind.annotation.XmlRootElement;

@XmlRootElement(name = "IntakeGiftRelation")  //  入伙礼品关系表
public class IntakeGiftRelation {
    private String intakeGiftRelationId;  // 入伙礼品关系id

    private String intakeId;  // 入伙活动id

    private String giftId;  // 礼品id

    private String buildingStructureId;  // 建筑结构id

    public String getIntakeGiftRelationId() {
        return intakeGiftRelationId;
    }

    public void setIntakeGiftRelationId(String intakeGiftRelationId) {
        this.intakeGiftRelationId = intakeGiftRelationId;
    }

    public String getIntakeId() {
        return intakeId;
    }

    public void setIntakeId(String intakeId) {
        this.intakeId = intakeId;
    }

    public String getGiftId() {
        return giftId;
    }

    public void setGiftId(String giftId) {
        this.giftId = giftId;
    }

    public String getBuildingStructureId() {
        return buildingStructureId;
    }

    public void setBuildingStructureId(String buildingStructureId) {
        this.buildingStructureId = buildingStructureId;
    }
}