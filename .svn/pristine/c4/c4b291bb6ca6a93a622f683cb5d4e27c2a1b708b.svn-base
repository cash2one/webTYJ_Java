                    <div class="panel" style="height: 250px">
                        <div class="panel_title">
                            <span class="panel_title_left">新建信息： </span>
                            <span class="panel_title_right"><span class="zoom_button"></span></span>
                            <span class="close_button"></span>
                        </div>
                        <div class="panel_info" style="margin-top: -20px">
                            <div class=" panel_info clear">
                                <div class="btn-group col-md-offset-11"></div>
                            </div>
                            <div align="center" style="padding:30px"><a data-toggle="modal"   data-target="#myModal2"> <img class="img3" src="images/staffHome/cmm1_u155.png" style="width: 100px;height: 80px"></a></div>
                        </div>
                    </div>
                </a>
            </div>




            <div class="modal fade" id="myModal2" tabindex="-1" role="dialog">
                <div class="modal-dialog"  style="height:400px;width:760px;">
                    <div class="modal-content" >
                        <div class="panel_title">
                            <span class="panel_title_left">新建信息：</span>
                            <span class="panel_title_right"><span class="zoom_button"></span><span class="close_button" data-dismiss="modal" aria-hidden="true"></span></span>
                        </div>
                        <div class="panel_info" style="padding-top: 20px;" >
