<!--
    创建人：倪明
    页面名称：人员主页
    js路径：js/controllers/propertyService/staffHome
-->
<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>员工主页</title>
    <link rel="stylesheet" href="css/personnel.css">
</head>
<body ng-controller="staffCtrl">
<div class="row">
    <!--人员主页开始-->
    <div class="complaint_user">
        <div>
            <!--人员主页客户详细信息开始-->
            <div class="complaint_user_img"><img src="images/propertyService/user.jpg" width="100" height="100"></div>
            <div class="complaint_user_info">
                <div class="complaint_user_info_info">
                    <div class="complaint_user_info_icon">
                        <img src="images/propertyService/VIP.png" width="30" height="20"><br>
                        <img src="images/propertyService/red.png" width="20" height="15">
                        <img src="images/propertyService/yel.png" width="20" height="15">
                        <img src="images/propertyService/blue.png" width="20" height="15">
                    </div>
                    <table class="complaint_user_info_userinfo">
                        <tr>
                            <td class="col-md-4">客户姓名：{{personCust.name}}</td>
                            <td class="col-md-4">客户编号：{{personCust.custCode}}</td>
                            <td class="col-md-4">业主关系：{{personCust.owner}}</td>
                        </tr>
                        <tr>
                            <td class="col-md-4">物业费：{{personCust.propertyCosts}}</td>
                            <td class="col-md-4">停车卡：{{personCust.parkingCard}}</td>
                            <td class="col-md-4">停车位：{{personCust.parking}}</td>
                        </tr>
                        <tr>
                            <td class="col-md-4">年龄：{{personCust.age}}</td>
                            <td class="col-md-4">性别：{{personCust.sex}}</td>
                            <td class="col-md-4">亲属关系：{{personCust.relationship}}</td>
                        </tr>
                        <tr>
                            <td class="col-md-4">身份证号：{{personCust. IDCard}}</td>
                            <td class="col-md-4">联系电话：{{personCust.telephone}}</td>
                            <td class="col-md-4"></td>
                        </tr>
                    </table>
                </div>
            </div>
            <!--人员主页客户详细信息结束-->
        </div>
        <div class="clear"></div>
    </div>
    <div class="complaint_user">
        <div>
            <!--人员主页导航开始-->
            <div class="complaint_user_info_navigation" style="margin-left: 5px">
                <a class="btn" ui-sref="index.propertyService.staffHome.commonUser" ng-class="{'complaint__navigation':btnIndex==1}" ng-click="doClick(1)">常用</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.serviceRequest" ng-class="{'complaint__navigation ':btnIndex==2}" ng-click="doClick(2)">服务请求</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.empowerManagement.history" ng-class="{'complaint__navigation ':btnIndex==3}" ng-click="doClick(3)">授权管理</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.joinGang.choiceHousing" ng-class="{'complaint__navigation ':btnIndex==4}" ng-click="doClick(4)">入伙</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.parkingManagement" ng-class="{'complaint__navigation ':btnIndex==5}" ng-click="doClick(5)">车位管理</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.decorate.history" ng-class="{'complaint__navigation ':btnIndex==6}" ng-click="doClick(6)">装修</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.vehicleManagement.vehicleManagementCheck" ng-class="{'complaint__navigation ':btnIndex==7}" ng-click="doClick(7)">车辆管理</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.parkingCard.historyRecord" ng-class="{'complaint__navigation ':btnIndex==8}" ng-click="doClick(8)">停车卡</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.entranceGuard.checkEntranceGuard" ng-class="{'complaint__navigation ':btnIndex==9}" ng-click="doClick(9)">门禁卡</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.changeTitle" ng-class="{'complaint__navigation ':btnIndex==10}" ng-click="doClick(10)">产权变更</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.petRegistration.history" ng-class="{'complaint__navigation ':btnIndex==11}" ng-click="doClick(11)">宠物登记</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.lease.leaseAdd" ng-class="{'complaint__navigation ':btnIndex==12}" ng-click="doClick(12)">租赁</a>
                <a class="btn" ui-sref="index.propertyService.staffHome.releasePass.addReleasePass" ng-class="{'complaint__navigation ':btnIndex==13}" ng-click="doClick(13)">放行条</a>
            </div>
            <!--人员主页导航结束-->
            <div class="complaint_user_info1">
                <div class="complaint_user_info_button">
                    <span class="complaint_panel_info_navigation">
                        <ul>
                            <li><img src="images/propertyService/jiantoua.png" width="35" height="29">
                                <ul>
                                    <li class="program_panel_info_navigation_top">办理放行条</li>
                                    <li>办理门禁卡</li>
                                    <li>宠物登记</li>
                                    <li class="program_panel_info_navigation_bottom">缴纳物业费</li>
                                </ul>
                            </li>
                        </ul>
                    </span>
                </div>
            </div>
        </div>
        <div class="clear"></div>
    </div>
    <!--人员主页结束-->
</div>
<div class="row">
    <!--人员主页导航内容视图加载开始-->
    <div ui-view></div>
    <!--人员主页导航内容视图加载结束-->
</div>
</body>
</html>