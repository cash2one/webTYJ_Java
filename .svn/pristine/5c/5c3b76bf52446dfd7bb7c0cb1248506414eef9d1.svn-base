package com.flf.entity;

import javax.xml.bind.annotation.XmlRootElement;

/**
 * 八.	施工单位提交资料记录表
 * @author 陶勇超
 *  2015-8-31
 */
@XmlRootElement(name="ConstructionDiscloseInformation")
public class ConstructionDiscloseInformation {
	private String fileId; //文件Id
	private String applyId; //施工申请关联Id
	private Integer fileType; //文件类型    1消防批文  2装修图纸  3动火协议 4合同文件  5其它
	private String fileName; //文件名称
	private String fileAddress; //文件地址
	public String getFileId() {
		return fileId;
	}
	public void setFileId(String fileId) {
		this.fileId = fileId;
	}
	public String getApplyId() {
		return applyId;
	}
	public void setApplyId(String applyId) {
		this.applyId = applyId;
	}
	public Integer getFileType() {
		return fileType;
	}
	public void setFileType(Integer fileType) {
		this.fileType = fileType;
	}
	public String getFileName() {
		return fileName;
	}
	public void setFileName(String fileName) {
		this.fileName = fileName;
	}
	public String getFileAddress() {
		return fileAddress;
	}
	public void setFileAddress(String fileAddress) {
		this.fileAddress = fileAddress;
	}
	
}
